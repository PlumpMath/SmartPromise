@model ProfileViewModel

<div class="modal fade" id="_transaction_history_modal" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Transaction history</h4>
                <small class="text-muted">@Model.Address</small>
            </div>
            <div align="center" class="container-fluid">
                    <div class="loader" id="_modal_tx_loader"></div>
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>Block</th>
                                <th>Transaction id</th>
                            </tr>
                        </thead>
                        <tbody id="_transactions_table_content">
                            
                        </tbody>
                    </table>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>